{
  "manifest_version": 3,
  "name": "Maintainer Extension",
  "description": "rewrite header for maintenance",
  "version": "0.1",
  "declarative_net_request" : {
    "rule_resources" : [{
      "id": "ruleset",
      "enabled": true,
      "path": "rules.json"
    }]
  },
  "permissions": ["declarativeNetRequest"],
  "host_permissions": [
    "https://www.example.com/*",
    "https://api.example.com/*"
  ]
}

